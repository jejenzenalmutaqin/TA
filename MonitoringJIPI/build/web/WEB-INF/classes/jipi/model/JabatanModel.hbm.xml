<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="jipi.model.JabatanModel" table="jabatan_tbl">
        <id name="kdjabatan" column="kdjabatan">
            <generator class="assigned"/>
        </id>
        <property name="namajabatan" column="namajabatan" length="50"/>
    </class>
</hibernate-mapping>
