<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="jipi.model.JurusanModel" table="mst_jurusan">
        <id name="kdjurusan" column="kdjurusan">
            <generator class="assigned"/>
        </id>
        <property name="namajurusan" column="namajurusan" length="50"/>
        <property name="kdfakultas" column="kdfakultas"/>
    </class>

</hibernate-mapping>
