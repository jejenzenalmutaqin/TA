<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="jipi.model.SidangModel" table="pengajuansidang_tbl">
        <id name="kdsidang" column="kdsidang" length="8">
            <generator class="assigned"/>
        </id>
        <property name="kdjenissidang" column="kdjenissidang"/>
        <property name="nim" column="nim"/>
        <property name="kdproposal" column="kdproposal"/>
        <property name="tglpengajuansidang" column="tglpengajuansidang" />
        <property name="penelaah1" column="penelaah1" />
        <property name="penelaah2" column="penelaah2" />
        <property name="wakilfakultas" column="wakilfakultas" length="50"/>
        <property name="jamsidang" column="jamsidang"/>
        <property name="tglsidang" column="tglsidang" />
        <property name="ruangansidang" column="ruangansidang" length="50"/>
    </class> 
</hibernate-mapping>
