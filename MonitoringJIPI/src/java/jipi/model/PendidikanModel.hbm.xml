<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="jipi.model.PendidikanModel" table="pendidikan_tbl">
        <id name="kdpendidikan" column="kdpendidikan">
            <generator class="assigned"/>
        </id>
        <property name="namapendidikan" column="namapendidikan" length="50"/>
    </class>
</hibernate-mapping>
