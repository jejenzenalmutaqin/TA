<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="jipi.model.ProposalModel" table="pengajuanproposal_tbl">
        <id name="kdproposal" column="kdproposal" length="8">
            <generator class="assigned"/>
        </id>
        <property name="kdjenisproposal" column="kdjenisproposal"/>
<!--        <many-to-one name="kdjenisproposal" column="kdjenisproposal" class="jipi.model.JenisPengajuanModel" not-null="true"/>-->
        <property name="nim" column="nim"/>
        <property name="judulproposal" column="judulproposal"/>
        <property name="perubahanjudul" column="perubahanjudul" />
        <property name="skssudahtempuh" column="skssudahtempuh" length="4"/>
        <property name="sksproposal" column="sksproposal" length="4"/>
        <property name="tglpengajuanproposal" column="tglpengajuanproposal" length="50"/>
        <property name="ipk" column="ipk"/>
        <property name="statusproposal" column="statusproposal" length="50"/>
        <property name="dosenpembimbing" column="dosenpembimbing" length="50"/>
        <property name="email" column="email" length="50"/>
    </class> 

</hibernate-mapping>
